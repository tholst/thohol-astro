{
  "name": "thohol-astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "dev:docker": "astro dev --host",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "qa": "npm run check && npm run lint && npm run format:check && npm run build",
    "qa:fix": "npm run lint:fix && npm run format && npm run check",
    "prepare": "husky",
    "screenshot": "node scripts/screenshot.js",
    "screenshot:all": "node scripts/screenshot-all.js",
    "screenshots": "node scripts/screenshot-all.js",
    "screenshot-homepage": "node scripts/screenshot-homepage.js"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.13",
    "@astrojs/rss": "^4.0.15",
    "@astrojs/sitemap": "^3.7.0",
    "astro": "^5.18.0",
    "sharp": "^0.34.2"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@eslint/js": "^9.28.0",
    "@playwright/test": "^1.42.1",
    "eslint": "^9.28.0",
    "eslint-plugin-astro": "^1.3.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  },
  "lint-staged": {
    "*.{astro,js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,json,css}": [
      "prettier --write"
    ]
  }
}
